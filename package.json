{
  "name": "add-to-milestone-action",
  "version": "0.0.0",
  "private": true,
  "description": "Add PR to a milestone",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --minify --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run lint && npm test && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrefcdias/add-to-milestone.git"
  },
  "keywords": [
    "actions",
    "pr",
    "milestone"
  ],
  "author": "andrefcdias",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.4.0",
    "@actions/github": "5.0.0",
    "minimatch": "3.0.4"
  },
  "devDependencies": {
    "@types/node": "^16.4.6",
    "@types/jest": "^26.0.24",
    "@types/minimatch": "^3.0.5",
    "@typescript-eslint/parser": "^4.28.3",
    "@vercel/ncc": "^0.29.0",
    "casual": "^1.6.2",
    "eslint": "^7.31.0",
    "eslint-plugin-github": "^4.1.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "js-yaml": "^4.1.0",
    "prettier": "2.3.2",
    "ts-jest": "^27.0.3",
    "typescript": "^4.1.3"
  }
}
