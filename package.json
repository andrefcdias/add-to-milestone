{
  "name": "add-to-milestone-action",
  "version": "1.2.2",
  "private": true,
  "description": "Add PR to a milestone",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --minify --license licenses.txt",
    "test": "jest",
    "all": "yarn build && yarn format && yarn lint && yarn test && yarn package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrefcdias/add-to-milestone.git"
  },
  "keywords": [
    "actions",
    "pr",
    "milestone"
  ],
  "author": "andrefcdias",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.6.0",
    "@actions/github": "5.0.0",
    "minimatch": "3.0.4"
  },
  "devDependencies": {
    "@octokit/webhooks-types": "5.4.0",
    "@types/jest": "27.4.1",
    "@types/minimatch": "3.0.5",
    "@types/node": "17.0.21",
    "@typescript-eslint/parser": "5.27.0",
    "@vercel/ncc": "0.33.3",
    "casual": "1.6.2",
    "eslint": "8.8.0",
    "eslint-plugin-github": "4.3.5",
    "eslint-plugin-jest": "26.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "jest": "27.5.1",
    "js-yaml": "4.1.0",
    "prettier": "2.5.1",
    "ts-jest": "27.1.3",
    "typescript": "4.5.5",
    "yarn": "1.22.17"
  }
}