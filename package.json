{
  "name": "add-to-milestone-action",
  "version": "1.3.0",
  "private": true,
  "description": "Add PR to a milestone",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:check": "tsc -p tsconfig.json --noEmit",
    "format": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --minify --license licenses.txt",
    "test": "jest",
    "all": "yarn build && yarn format && yarn lint && yarn test && yarn package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrefcdias/add-to-milestone.git"
  },
  "keywords": [
    "actions",
    "pr",
    "milestone"
  ],
  "author": "andrefcdias",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.10.0",
    "@actions/github": "5.1.1",
    "minimatch": "5.1.0"
  },
  "devDependencies": {
    "@octokit/webhooks-types": "6.4.0",
    "@types/jest": "29.1.1",
    "@types/minimatch": "5.1.2",
    "@types/node": "18.8.0",
    "@typescript-eslint/parser": "5.46.1",
    "@vercel/ncc": "0.34.0",
    "casual": "1.6.2",
    "eslint": "8.24.0",
    "eslint-plugin-github": "4.3.7",
    "eslint-plugin-jest": "27.0.4",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.1.2",
    "js-yaml": "4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "29.0.3",
    "typescript": "4.8.4"
  }
}