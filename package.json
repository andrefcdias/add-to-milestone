{
  "name": "add-to-milestone-action",
  "version": "1.2.4",
  "private": true,
  "description": "Add PR to a milestone",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "build:check": "tsc -p tsconfig.json --noEmit",
    "format": "prettier --write **/*.ts",
    "format:check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --minify --license licenses.txt",
    "test": "jest",
    "all": "yarn build && yarn format && yarn lint && yarn test && yarn package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andrefcdias/add-to-milestone.git"
  },
  "keywords": [
    "actions",
    "pr",
    "milestone"
  ],
  "author": "andrefcdias",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.8.2",
    "@actions/github": "5.0.3",
    "minimatch": "5.1.0"
  },
  "devDependencies": {
    "@octokit/webhooks-types": "6.2.2",
    "@types/jest": "28.1.4",
    "@types/minimatch": "3.0.5",
    "@types/node": "18.0.3",
    "@typescript-eslint/parser": "5.30.6",
    "@vercel/ncc": "0.34.0",
    "casual": "1.6.2",
    "eslint": "8.19.0",
    "eslint-plugin-github": "4.3.6",
    "eslint-plugin-jest": "26.5.3",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "28.1.2",
    "js-yaml": "4.1.0",
    "prettier": "2.7.1",
    "ts-jest": "28.0.5",
    "typescript": "4.7.4"
  }
}